<%= form_with(model: @venue, local: true) do |f| %>

  <div class="form-group">
    <%= f.label :address %>
    <%= f.text_area :address, rows: "3", class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :dish_id %>

    <%= f.text_field :dish_id, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :google_maps_url %>

    <%= f.text_field :google_maps_url, class: "form-control" %>
  </div>

  <%= f.hidden_field :neighborhood_id, value: @neighborhood.id %>

  <%= f.submit "Create venue", class: "btn btn-block btn-outline-secondary" %>
<% end %>
