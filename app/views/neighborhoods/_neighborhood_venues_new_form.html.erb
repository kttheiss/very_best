<!-- Form venue start -->
<%= form_with(model: @venue, local: true) do |f| %>
<!-- Form venue end -->

  <!-- Label and input for address start -->
  <div class="form-group">
    <%= f.label :address %>
    <!-- Input for address start -->
    <%= f.text_area :address, rows: "3", class: "form-control" %>
    <!-- Input for address end -->
  </div>
  <!-- Label and input for address end -->

  <!-- Label and input for dish_id start -->
  <div class="form-group">
    <%= f.label :dish_id %>

    <!-- Input for dish_id start -->
    <%= f.text_field :dish_id, class: "form-control" %>
    <!-- Input for dish_id end -->
  </div>
  <!-- Label and input for dish_id end -->

  <!-- Label and input for google_maps_url start -->
  <div class="form-group">
    <%= f.label :google_maps_url %>

    <!-- Input for google_maps_url start -->
    <%= f.text_field :google_maps_url, class: "form-control" %>
    <!-- Input for google_maps_url end -->
  </div>
  <!-- Label and input for google_maps_url end -->

  <!-- Label and input for neighborhood_id start -->
  <!-- Input for neighborhood_id start -->
  <%= f.hidden_field :neighborhood_id, value: @neighborhood.id %>
  <!-- Input for neighborhood_id end -->
  <!-- Label and input for neighborhood_id end -->

  <%= f.submit "Create venue", class: "btn btn-block btn-outline-secondary" %>
<% end %>
